; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="PiggyBeat Dash"
run/main_scene="res://TitleScreen.tscn"
config/icon="res://icon.png"

[autoload]

UserData="*res://addons/github-integration/scripts/user_data.gd"
IconLoaderGithub="*res://addons/github-integration/scripts/IconLoaderGithub.gd"
RestHandler="*res://addons/github-integration/scripts/RestHandler.gd"

[display]

window/size/width=380
window/size/height=220
window/size/test_width=1200
window/size/test_height=750
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "github-integration" )

[rendering]

quality/2d/use_pixel_snap=true
environment/default_environment="res://default_env.tres"
